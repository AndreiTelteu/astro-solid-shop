---
import MainHead from '~/components/MainHead.astro';
import Header from '~/components/Header/Header.astro';

const { head } = Astro.props;

---
<html lang={'en'}>
  <head>
    <MainHead title={head.title} description={head.description} />
  </head>
  <body>
    <Header />
    <slot />
  </body>
</html>
